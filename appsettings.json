{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
        "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
      }
    },
    "WriteTo:File": {
      "Name": "File",
      "Args": {
        "path": "logs/log.log",
        "rollingInterval": "Month",
        "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Destructure": [],
    "Properties": {
      "Application": "SPS System Logs"
    }
  },
  "ConnectionStrings": {
    "db": "Host=localhost;Database=SIPS.Connect.DB;Include Error Detail=True;Username=postgres;Password={{PASSWORD}};"
  },
  "AllowedHosts": "*",
  "Core": {
    "BaseUrl": "http://localhost:5004/api",
    "PublicKeysRepUrl": "/v1/Certificates/Download",
    "LoginEndpoint": "/v1/auth/login",
    "Username": "",
    "Password": "",
    "BIC": "",
    "SAFExpression": "* 0 * * *",
    "SAFPage": 40,
    "SAFTimeZoneInfo": "Utc",
    "SAFMaxRetries": 10
  },
  "Xades": {
    "CertificatePath": "./certs/certificate.pem",
    "PrivateKeyPath": "./certs/private.key",
    "PrivateKeyPassphrase": null,
    "ChainPath": "./certs/chain.pem",
    "Algorithms": [
      "SHA1withRSA",
      "SHA256withRSA"
    ],
    "VerificationWindowMinutes": 1000,
    "BIC": "AGROSOS0",
    "WithoutPKI": true,
    "DefaultSignatureMethod": "SHA1withRSA"
  },
  "ISO20022": {
    "Verification": "http://localhost:5025/api/cb/verify",
    "Transfer": "http://localhost:5025/api/CB/Transfer",
    "Return": "http://localhost:5025/api/CB/Return",
    "Status": "http://localhost:5025/api/CB/Status",
    "SIPS": "http://localhost:8094/api/v1/incoming",
    "BIC": "AGROSOS0",
    "Agent": "AGROSOS0",
    "Key": "Very_Secret_Key",
    "Secret": "Very_Secret_Secret"
  },
  "Emv": {
    "AcquirerId": "0010",
    "FIType": "01",
    "FIName": "AGRO Bank",
    "Version": "01",
    "CountryCode": "SO",
    "Tags": {
      "MerchantIdentifier": 26,
      "AcquirerTag": 1,
      "MerchantIdTag": 44
    }
  },
  "Keycloak": {
    "Realm": {
      "Host": "idp:8443",
      "Protocol": "https",
      "Name": "mgt",
      "Audience": "sc-api"
    }
  },
  "CorsPolicies": {
    "Origins": [
      "https://localhost:3000"
    ]
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "https://0.0.0.0:8080"
      },
      "Https": {
        "Url": "https://0.0.0.0:443",
        "Certificate": {
          "Path": "./certs/sips-connect.pfx",
          "Password": "YOUR_PASSWORD"
        }
      }
    }
  },
  "ApiKeys": [
    {
      "Key": "123",
      "Secret": "123",
      "Name": "Test"
    }
  ]
}