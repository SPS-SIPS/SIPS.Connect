{
  "Serilog": {
    "Using": [
      "Serilog.Sinks.Console",
      "Serilog.Sinks.File"
    ],
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Information",
        "System": "Information"
      }
    },
    "WriteTo:Console": {
      "Name": "Console",
      "Args": {
        "theme": "Serilog.Sinks.SystemConsole.Themes.AnsiConsoleTheme::Code, Serilog.Sinks.Console",
        "outputTemplate": "[{Timestamp:HH:mm:ss} {Level:u3}] {Message:lj}{NewLine}{Exception}"
      }
    },
    "WriteTo:File": {
      "Name": "File",
      "Args": {
        "path": "logs/log.log",
        "rollingInterval": "Month",
        "formatter": "Serilog.Formatting.Json.JsonFormatter, Serilog"
      }
    },
    "Enrich": [
      "FromLogContext",
      "WithMachineName"
    ],
    "Destructure": [],
    "Properties": {
      "Application": "SPS System Logs"
    }
  },
  "ConnectionStrings": {
    "db": "Host=localhost;Database=SIPS.Connect.DB;Include Error Detail=True;Username=postgres;Password={{PASSWORD}};"
  },
  "AllowedHosts": "*",
  "Core": {
    "BaseUrl": "",
    "PublicKeysRepUrl": "",
    "LoginEndpoint": "",
    "LiveParticipantsEndpoint": "",
    "BalanceStatusEndpoint": "",
    "Username": "",
    "Password": "",
    "BIC": "",
    "SAFExpression": "*/1 * * * *",
    "SAFPage": 40,
    "SAFTimeZoneInfo": "Utc",
    "SAFMaxRetries": 10,
    "IncludeIdempotencyHeaders": false,
    "HttpTimeoutSeconds": 15,
    "DbPersistTimeoutSeconds": 10,
    "IncludeCoreBankOnListing": false,
    "TransactionTimeoutMinutes": 10,
    "TimeoutWorkerSchedule": "*/15 * * * *"
  },
  "Xades": {
    "CertificatePath": "./certs/certificate.pem",
    "PrivateKeyPath": "./certs/private.key",
    "PrivateKeyPassphrase": "",
    "ChainPath": "./certs/chain.pem",
    "Algorithms": [
      "SHA1withRSA",
      "SHA256withRSA"
    ],
    "VerificationWindowMinutes": 1000,
    "BIC": "ZKBASOS0",
    "WithoutPKI": true,
    "DefaultSignatureMethod": "SHA256withRSA",
    "BaseDN": "CN=sps issuing CA, DC=sps, DC=local"
  },
  "ISO20022": {
    "Verification": "http://localhost:5025/api/cb/verify",
    "Transfer": "http://localhost:5025/api/CB/Transfer",
    "Return": "http://localhost:5025/api/CB/Return",
    "Status": "http://localhost:5025/api/CB/Status",
    "SIPS": "http://localhost:8081/api/v1/incoming",
    "CompletionNotification": "http://localhost:5025/api/CB/CompletionNotification",
    "HealthCheck": "http://localhost:5025/api/CB/HealthCheck",
    "BIC": "ZKBASOS0",
    "Agent": "ZKBASOS0",
    "Key": "Very_Secret_Key",
    "Secret": "Very_Secret_Secret"
  },
  "Emv": {
    "AcquirerId": "0000",
    "FIType": "01",
    "FIName": "Bank Name",
    "Version": "01",
    "CountryCode": "SO",
    "Tags": {
      "MerchantIdentifier": 26,
      "AcquirerTag": 1,
      "MerchantIdTag": 44
    }
  },
  "Keycloak": {
    "Realm": {
      "Host": "idp:8443",
      "Protocol": "https",
      "Name": "mgt",
      "Audience": "sc-api"
    }
  },
  "CorsPolicies": {
    "Origins": [
      "http://localhost:3000"
    ]
  },
  "LiveParticipants": {
    "CacheDurationMinutes": 5
  },
  "Kestrel": {
    "Endpoints": {
      "Http": {
        "Url": "http://0.0.0.0:8081"
      },
      "Https": {
        "Url": "https://0.0.0.0:443",
        "Certificate": {
          "Path": "./certs/sips-connect.pfx",
          "Password": "YOUR_PASSWORD"
        }
      }
    }
  },
  "ApiKeys": [
    {
      "Key": "123",
      "Secret": "123",
      "Name": "Test"
    }
  ]
}